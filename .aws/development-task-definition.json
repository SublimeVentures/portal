{
    "containerDefinitions": [
        {
            "name": "webapp-dev",
            "image": "null",
            "cpu": 2048,
            "memory": 4096,
            "portMappings": [
                {
                    "name": "webapp-dev-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "dockerLabels": {
                "app": "webapp",
                "env": "dev",
                "project": "sublime"
            },
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/webapp-dev-td",
                    "awslogs-region": "eu-west-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "webapp-dev-td",
    "taskRoleArn": "arn:aws:iam::999673149159:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::999673149159:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": ["FARGATE"],
    "tags": [
        {
            "key": "project",
            "value": "-"
        },
        {
            "key": "env",
            "value": "develop"
        },
        {
            "key": "app",
            "value": "webapp"
        }
    ],
    "cpu": "1024",
    "memory": "3072",
    "ephemeralStorage": {
        "sizeInGiB": 21
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
