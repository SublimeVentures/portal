{
  "containerDefinitions": [
    {
      "name": "binarite-development-api-container",
      "image": "655401350744.dkr.ecr.us-east-2.amazonaws.com/binarite-development-api:3faa1e9",
      "cpu": 1024,
      "memory": 1024,
      "portMappings": [
        {
          "name": "binarite-api-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        },
        {
          "name": "binarite-api-443-tcp",
          "containerPort": 443,
          "hostPort": 443,
          "protocol": "tcp",
          "appProtocol": "http"
        },
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        },
        {
          "containerPort": 8081,
          "hostPort": 8081,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "ENV_TEST",
          "value": "ENV_TEST_VALUE"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "dockerLabels": {
        "app": "api",
        "environment": "development",
        "project": "binarite"
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/binarite-development-api",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "family": "binarite-development-api",
  "networkMode": "host",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2"
  ],
  "tags": [
    {
      "key": "project",
      "value": "binarite"
    },
    {
      "key": "environment",
      "value": "develop"
    }
  ]
}
