{
    "containerDefinitions": [
        {
            "name": "based-prod-webapp",
            "image": "null",
            "cpu": 2048,
            "memory": 8192,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "dockerLabels": {
                "app": "webapp",
                "env": "production",
                "project": "sublime"
            },
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/based-prod-webapp",
                    "awslogs-region": "us-west-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "family": "based-prod-webapp-td",
    "taskRoleArn": "arn:aws:iam::999673149159:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::999673149159:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "status": "ACTIVE",
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "8192",
    "tags": [
        {
            "key": "App",
            "value": "webapp"
        },
        {
            "key": "Project",
            "value": "based"
        },
        {
            "key": "Environment",
            "value": "prod"
        }
    ],
    "ephemeralStorage": {
        "sizeInGiB": 21
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
